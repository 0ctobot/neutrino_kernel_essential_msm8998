/*
 * msm8998-htc-common: common configs of msm8998 HTC projects
 */

#include "msm8998-touch-muskie.dtsi"
#include "msm8998-key-muskie.dtsi"
#include "msm8998-audio-muskie.dtsi"

&i2c_6 {
	status = "disabled";
};

&i2c_7 {
	status = "okay";
};

&i2c_8 {
	status = "okay";
};

&i2c_9 {
	status = "okay";
};

&i2c_5_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_5_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_7_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_7_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_8_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_8_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_9_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_9_sleep {
	config {
		drive-strength = <8>;
	};
};

&soc {
	htc_radio_smem {
	    compatible = "htc,htc_radio_smem";
	};
};

&pil_slpi_mem {
	/* qct original reg = <0 0x94100000 0 0xf00000>; */
	reg = <0 0x94900000 0 0xf00000>;
};

&pil_video_mem {
	/* qct original reg = <0 0x93c00000 0 0x500000>; */
	reg = <0 0x94400000 0 0x500000>;
};

&pil_ipa_gpu_mem {
	/* qct original reg = <0 0x95000000 0 0x100000>; */
	reg = <0 0x95800000 0 0x100000>;
};

&modem_mem {
	/* qct original reg = <0 0x8cc00000 0 0x7000000>; */
	reg = <0 0x8cc00000 0 0x7800000>;
};

/ {
	reserved-memory {
		ramoops_mem: ramoops_region {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x83100000 0 0x00200000>;
		};
	};

	ramoops {
		compatible = "ramoops";
		memory-region = <&ramoops_mem>;
		record-size = <0 0x20000>; /* 128 KB */
		console-size = <0 0x100000>; /* 1024 KB */
		pmsg-size = <0 0x80000>; /* 512 KB */
	};
};
