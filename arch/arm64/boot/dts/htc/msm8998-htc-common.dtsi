/*
 * msm8998-htc-common: common configs of msm8998 HTC projects
 */

#include "msm8998-touch-muskie.dtsi"
#include "msm8998-key-muskie.dtsi"
#include "msm8998-audio-muskie.dtsi"
#include "msm8998-htc-fingerprint-muskie.dtsi"

&i2c_6 {
	status = "disabled";
};

&i2c_7 {
	status = "okay";
};

&i2c_8 {
	status = "okay";
};

&i2c_9 {
	status = "okay";
};

&i2c_5_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_5_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_7_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_7_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_8_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_8_sleep {
	config {
		drive-strength = <8>;
	};
};

&i2c_9_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_9_sleep {
	config {
		drive-strength = <8>;
	};
};

&tlmm {
	camera_i2c_switch_ctrl {
		switch_ctrl_active: switch_ctrl_active {
			mux {
				pins = "gpio30";
				function = "gpio";
			};

			config {
				pins = "gpio30";
				drive-strength = <2>;
				output-high;
			};
		};
	};
};


&soc {
	qcom,msm-ssc-sensors {
		pinctrl-names = "default";
		pinctrl-0 = <&switch_ctrl_active>;
	};
	htc_radio_smem {
	    compatible = "htc,htc_radio_smem";
	};
};

&ssc_sensors {
	qcom,firmware-name = "slpi";
};

&pmi8998_gpios {
	gpio@c100 {
		reg = <0xc100 0x100>;
		qcom,pin-num = <2>;
		status = "okay";
		qcom,mode = <1>; // Digital output
		qcom,invert = <1>;
		qcom,pull = <5>; // No Pull
		qcom,vin-sel = <0>; // 1.8 V, VIN0
		qcom,src-sel = <3>;
		qcom,master-en = <1>; // enable GPIO
		qcom,out-strength = <2>; // Output strength medium
	};
};

&pil_slpi_mem {
	/* qct original reg = <0 0x93800000 0 0xf00000>; */
	reg = <0 0x93a00000 0 0xf00000>;
};

&pil_video_mem {
	/* qct original reg = <0 0x93300000 0 0x500000>; */
	reg = <0 0x93500000 0 0x500000>;
};

&pil_adsp_mem {
	/* qct original reg = <0 0x91900000 0 0x1a00000>; */
	reg = <0 0x91b00000 0 0x1a00000>;
};

&modem_mem {
	/* qct original reg = <0 0x8ac00000 0 0x6d00000>; */
	reg = <0 0x8ac00000 0 0x6f00000>;
};
