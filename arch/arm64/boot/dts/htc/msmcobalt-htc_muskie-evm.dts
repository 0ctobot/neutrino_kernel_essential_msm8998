/dts-v1/;

#include "../qcom/msmcobalt.dtsi"
#include "../qcom/msmcobalt-mtp.dtsi"
#include "msmcobalt-htc_muskie.dtsi"
#include "msmcobalt-camera-sensor.dtsi"

/ {
	model = "HTC Corporation. MSM cobalt v1 Muskie EVM";
	compatible = "qcom,msmcobalt-mtp", "qcom,msmcobalt", "qcom,mtp";
	qcom,board-id = <0x1D 0>;
};

&i2c_9 {
	tca6418_i2c: tca6418-i2c@34 {
		compatible = "ti,tca6418-i2c";
		reg = <0x34>;
		pinctrl = <&tca6418>;
	};
};

&soc {
	tca6418: tca6418-pinctrl {
		compatible = "ti,tca6418-pinctrl";
		#gpio-cells = <2>;
		label = "tca6418-gpio";
		i2c-client = <&tca6418_i2c>;
	 };
};
