&soc {
	tlmm: pinctrl@03000000 {
		compatible = "qcom,msmfalcon-pinctrl";
		reg = <0x03000000 0xc00000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;

		uart_console_active: uart_console_active {
			mux {
				pins = "gpio0", "gpio1";
				function = "blsp_uart1";
			};

			config {
				pins = "gpio0", "gpio1";
				drive-strength = <2>;
				bias-disable;
			};
		};

		led_enable: led_enable {
			mux {
				pins = "gpio40";
				drive_strength = <16>;
				output-high;
			};
		};

		led_disable: led_disable {
			mux {
				pins = "gpio40";
				drive_strength = <2>;
				output-low;
			};
		};
	};
};
